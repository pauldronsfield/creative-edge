<%
const header = {
	hasGradient: false,
	hasWhiteBackground: true
}
const imagePaths = [
'images/business_portraits/business_portraits_1.jpg',
'images/business_portraits/business_portraits_2.jpg',
'images/business_portraits/business_portraits_3.jpg',
'images/business_portraits/business_portraits_4.jpg',
'images/business_portraits/business_portraits_5.jpg',
'images/business_portraits/business_portraits_6.jpg',
'images/business_portraits/business_portraits_7.jpg',
'images/business_portraits/business_portraits_8.jpg',
'images/business_portraits/business_portraits_9.jpg',
'images/business_portraits/business_portraits_10.jpg',
'images/business_portraits/business_portraits_11.jpg',
'images/business_portraits/business_portraits_12.jpg'
]
%>

<div class="container business-portraits-page">

	<div class="row">
		<div class="columns twelve">

			<%- partial('../components/header.ejs', { header }) %>

		</div>
	</div>

	<div class="row gallery__page-top u-margin-b--lrg">
		<div class="columns eight offset-by-two twelve--med offset-by-none--med">

			<div id="gallery" style="display:none;">

				<% imagePaths.forEach(function(imagePath) { %>

					<img alt="Image 1 Title" src="<%= imagePath %>"
					data-image="<%= imagePath %>"
					data-description="Image 1 Description">

				<% }) %>

			</div>

		</div>
	</div>

	<div class="row">
		<div class="columns twelve">

			<%- partial('../components/footer.ejs') %>

		</div>
	</div>

</div>
