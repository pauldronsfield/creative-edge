<%
const header = {
  hasGradient: false,
  hasWhiteBackground: true
}
const imagePaths = [
	'images/properties/properties_1.jpg',
	'images/properties/properties_2.jpg',
	'images/properties/properties_3.jpg',
	'images/properties/properties_4.jpg',
	'images/properties/properties_5.jpg',
	'images/properties/properties_6.jpg',
	'images/properties/properties_7.jpg',
	'images/properties/properties_8.jpg',
	'images/properties/properties_9.jpg',
	'images/properties/properties_10.jpg',
	'images/properties/properties_11.jpg',
	'images/properties/properties_12.jpg'
]
%>

<div class="container properties-page">

  <div class="row">
    <div class="columns twelve">
      
      <%- partial('../components/header.ejs', { header }) %>

		</div>
	</div>

	<div class="row gallery__page-top u-margin-b--lrg">
		<div class="columns eight offset-by-two twelve--med offset-by-none--med">

			<div id="gallery" style="display:none;">

				<% imagePaths.forEach(function(imagePath) { %>

					<img alt="Image 1 Title"
						 src="<%= imagePath %>"
						 data-image="<%= imagePath %>"
						 data-description="Image 1 Description">

				<% }) %>

			</div>

		</div>
	</div>

	<div class="row">
	    <div class="columns twelve">
	      
	      <%- partial('../components/footer.ejs') %>

	    </div>
	  </div>

</div>